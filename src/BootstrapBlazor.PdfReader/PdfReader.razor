@namespace BootstrapBlazor.Components

<iframe @ref="Element"
        allow="fullscreen"
        style="@($"border: none; width: {Width}; height: {Height};")"
        src=@($"{ViewerBase}?file={System.Web.HttpUtility.UrlEncode(Filename)}#page={Page}?navpanes={Navpanes}&toolbar={Toolbar}&statusbar={Statusbar}&view={View}&pagemode={Pagemode}&search={Search}") />